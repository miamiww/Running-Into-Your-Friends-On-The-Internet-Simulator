{
	"manifest_version": 2,
	"name": "Running Into Your Friends On The Internet Simulator 2020",
	"version": "0.1",
	"background": {
	  "scripts": ["background.js"]
	},
	"content_scripts": [
	  {
		"matches": [
		  "<all_urls>"
		],
		"js": ["jquery.min.js", "content.js"]
	  }
	],
	"browser_action": {
	  "default_icon": "icon.png"
	},
	"permissions": [
		"<all_urls>",
		"tabs",
		"webRequest",
        "webRequestBlocking",
        "webNavigation"
	]
  }